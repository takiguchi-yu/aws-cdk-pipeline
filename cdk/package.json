{
  "private": true,
  "name": "aws-cdk-pipeline",
  "version": "1.0.0",
  "description": "Baseline for AWS Control Tower guest accounts",
  "license": "MIT-0",
  "bin": {
    "aws-cdk-pipeline": "bin/aws-cdk-pipeline.js"
  },
  "scripts": {
    "synth": "cdk synth -q",
    "deploy": "cdk deploy --require-approval never",
    "depcheck": "depcheck",
    "build": "tsc --build",
    "clean": "tsc --build --clean && rm -f parameter.js parameter.d.ts && rm -rf cdk.out",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "list": "cdk ls"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/node": "18.16.14",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "aws-cdk": "^2.90.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.0.4"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.81.0",
    "constructs": "^10.2.36"
  }
}
